<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/prime_blue_418FF6"/>
            <size android:width="@dimen/_18sdp"
                android:height="@dimen/_18sdp"/>
                <stroke android:width="@dimen/_1sdp"
                android:color="@color/blue_dfeefd"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <size android:width="@dimen/_18sdp"
                android:height="@dimen/_18sdp"/>
            <stroke android:width="@dimen/_1sdp"
                android:color="@color/prime_blue_418FF6"/>
        </shape>
    </item>
</selector>