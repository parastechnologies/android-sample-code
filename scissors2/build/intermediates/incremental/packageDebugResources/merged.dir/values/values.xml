<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorGreyProfile">#99CECECE</color>
    <declare-styleable name="CropView">
        <!-- Viewport ratio used to compute height based off view width -->
        <attr format="float" name="cropviewViewportRatio"/>
        <!-- Viewport overlay color -->
        <attr format="color" name="cropviewViewportOverlayColor"/>
        <!-- Viewport overlay padding -->
        <attr format="dimension" name="cropviewViewportOverlayPadding"/>
        <!-- Maximum zoom level -->
        <attr format="float" name="cropviewMaxScale"/>
        <!-- Minimum  zoom level -->
        <attr format="float" name="cropviewMinScale"/>
        <!-- Shape -->
        <attr format="enum" name="cropviewShape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
    </declare-styleable>
</resources>