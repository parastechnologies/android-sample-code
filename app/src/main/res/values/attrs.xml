<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseVisualizer">
        <attr name="avDensity" format="float"/>
        <attr name="avColor" format="color"/>
        <attr name="avType" format="string"/>
        <attr name="avGravity" format="string"/>
        <attr name="avWidth" format="dimension"/>
        <attr name="avSpeed" format="string"/>
    </declare-styleable>

    <declare-styleable name="GradientProgressBar">
        <attr name="gpb_color" format="string"/>
        <attr name="gpb_progress" format="float"/>
        <attr name="gpb_height" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="EditTextView">
        <attr name="enableTagging" format="boolean"/>
    </declare-styleable>

</resources>